      <% @projects_months.each do |month, item| %>
      <div class="panel panel-primary">
        <div class="panel-heading">
          <strong><%= month.strftime('%B, %Y') %></strong>
        </div>
        <div class="panel-body">
          <% for project in item %>
            <strong><%= link_to project.title, project %></strong>
            - <%= l(project.created_date, format: :shorter) %>
            <br />
          <% end %>
        </div>
      </div>
      <% end %>
